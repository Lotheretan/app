<div>
  {{#ui-form newValue=model  validation="save" cancel="cancel" bigIcon='user' smallIcon='plus' header='Developer' description='Adding an object'}}
    <div class="field">
      <label id="" class="" for="frmEdit-identity">Identity</label>
      <div id="div-frmEdit-identity" class="ui input">
        {{input id="frmEdit-identity" value=model.identity placeholder="Identity"}}
      </div>
      <label id="" class="" for="frmEdit-password">Password</label>
      <div id="div-frmEdit-password" class="ui input">
        {{input id="frmEdit-password" value=model.password placeholder="Password" type="password"}}
      </div>
    </div>
  {{/ui-form}}
</div>{{outlet}}